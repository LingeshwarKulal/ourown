{
  "buildCommand": "npm run vercel-build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "outputDirectory": "dist",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*\\.(js|css|svg|png|jpg|jpeg|gif|ico|json))", "dest": "/$1" },
    { "src": "/.*", "dest": "/index.html" }
  ]
} 